---
import Layout from '@layouts/Default.astro';
let title = 'Testimonials | Reviews of Foxbat Systems';
let description = 'Read testimonials and reviews for Foxbat Systems, your professionals preferred by local residents and businesses.';
---
<Layout title={title} description={description}>
<section class="row py-xl-5" id="general">
	<div class="col-xl-9 col-lg-10 col-12 mx-auto p-3 p-lg-5">
		<div id="cms-breadcrumb" class="minimal-breadcrumb d-lg-block d-none">
			<div class="trail"><a title="Home" href="/">Home</a></div>
			<div class="trail"><a title="Reviews" href="/reviews">Reviews</a></div>
		</div>
		<h1>Reviews and Testimonials for Foxbat Systems</h1>
		<div class="testimonials-container">
			<div class="row">
				<div class="col-12">
					<p>
						There are currently no reviews or
						testimonials; check back soon!
					</p>
				</div>
			</div>
		</div>
	</div>
</section>
</Layout>